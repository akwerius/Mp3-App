<h1>Filtered mp3s</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Title</th>
    <th>Artist</th>
    <th>Length</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @filtered_mp3s.each do |mp3| %>
  <tr>
    <td><%= mp3.url %></td>
    <td><%= mp3.title %></td>
    <td><%= mp3.artist %></td>
    <td><%= mp3.length %></td>
    <td><%= link_to 'Show', mp3 %></td>
    <td><%= link_to 'Edit', edit_mp3_path(mp3) %></td>
    <td><%= link_to 'Destroy', mp3, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br /> <font color="white">
params: <%= params.inspect %>
<br />
Mp3_Artist:  <%= params[:Mp3][:Artist].inspect %>
<br />
Mp3_Rating:  <%= params[:Mp3][:Rating].inspect %>
<br />
iMp3_Artist:  <%= params[:iMp3_Artist].inspect %>
<br />
filtered_mp3s: <%= @filtered_mp3s.inspect %>
<br />
session: <%= session.inspect %>
<br />
</font><br />
